{# app/Resources/views/registration/register.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
    <h2>Inscription</h2>

    {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.phoneNumber) }}

        <button type="submit" class="btn btn-primary">S'inscrire</button>
    {{ form_end(form) }}
{% endblock %}
